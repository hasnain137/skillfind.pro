{
    "extends": "next/core-web-vitals",
    "plugins": [
        "i18next"
    ],
    "rules": {
        "i18next/no-literal-string": [
            "warn",
            {
                "markupOnly": true,
                "ignoreAttribute": [
                    "className",
                    "href",
                    "src",
                    "alt",
                    "key",
                    "layout",
                    "priority"
                ]
            }
        ]
    }
}